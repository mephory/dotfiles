#!/usr/bin/env ruby
# curtab - print the currently active qutebrowser tab

require 'yaml'

session = YAML.safe_load(File.read(File.expand_path('~/.local/share/qutebrowser/sessions/_autosave.yml')))

session['windows']
  .select { |w| w['active'] }
  .flat_map { |w| w['tabs'] }
  .select { |t| t['active'] }
  .flat_map { |t| t['history'] }
  .select { |h| h['active'] }
  .each do |h|
    puts "#{h['url']}"
  end
