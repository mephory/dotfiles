#!/bin/sh
pactl unload-module module-echo-cancel
pactl load-module module-echo-cancel use_master_format=1 aec_method=webrtc aec_args="analog_gain_control=0\ digital_gain_control=1" source_name=echoCancel_source sink_name=echoCancel_sink source_master=alsa_input.pci-0000_00_1f.3.analog-stereo sink_master=alsa_output.pci-0000_00_1f.3.analog-stereo 
pacmd set-default-source echoCancel_source
# pacmd set-default-sink echoCancel_sink

